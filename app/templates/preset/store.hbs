<div class='page-content'>
  <h2>Podium Cam (2) &ndash; RS-30 USB &raquo; Store Preset</h2>

  <ul class='fields'>
    <li class='field'>
      <a href='#' class='btn'>Preset 1</a>
    </li>

    <li class='field'>
      <span class='switch'>
        <div class='switch-control off'>
          <span class='switch-state-label'>Off</span>
          <span class='switch-btn'></span>
        </div>
        <div class='switch-label'>Trisync</div>
      </span>
      <a href='#' class='btn btn-disabled'>Trisync Speed...</a>
    </li>

    <li class='field'>
      <span class='switch'>
        <div class='switch-control on'>
          <span class='switch-state-label'>On</span>
          <span class='switch-btn'></span>
        </div>
        <div class='switch-label'>Color Settings</div>
      </span>
    </li>
  </ul>
</div>

<div class='tiles page-nav'>
  {{#link-to 'preset' class='btn tile-btn'}}
    <span class='icon'>~icon~</span>
    <span class='text'>Save</span>
  {{/link-to}}

  {{#link-to 'preset' class='btn tile-btn'}}
    <span class='icon'>~icon~</span>
    <span class='text'>Back</span>
  {{/link-to}}
</div>
